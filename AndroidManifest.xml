<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.scanner"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.scanner.LauncherActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.scanner.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.scanner.NumberPickerActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.scanner.MobChooser"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.scanner.FunctionChooser"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.scanner.FunctionSwitcher"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.scanner.ScrollActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.scanner.CreateModActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
        </activity>
        <activity
            android:name="com.example.scanner.InfoActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.scanner.CustomArrayControllerActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.scanner.TestingScrollActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.scanner.CameraActivity"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
        </activity>
         <activity
            android:name="com.example.scanner.DialogActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>